{
    "name": "lumi",
    "version": "v0.1.1",
    "description": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/Lumieducation/Lumi"
    },
    "scripts": {
        "start": "npm run build && npm run dev",
        "test": "jest",
        "test:update": "jest -u",
        "clean": "rm -rf build && rm -rf dist",
        "uninstall": "npm run clean && rm -rf node_modules",
        "dev": "NODE_ENV=development VERSION=$(git describe --tags) ./node_modules/.bin/webpack-dev-server --config './config/webpack.config.dev.js' --progress --colors --hot --inline",
        "build": "npm run clean && npm run build:client && npm run build:server",
        "build:server": "npm run lint:server  && ./node_modules/.bin/tsc --project tsconfig.server.json &&  cp -r ./src/server/views ./build/src/server",
        "build:client": "NODE_ENV=production VERSION=$(git describe --tags) ./node_modules/.bin/webpack --config './config/webpack.config.prod.js' --progress --colors",
        "lint:client": "./node_modules/.bin/tslint --project tsconfig.json --config tslint.json",
        "lint:server": "./node_modules/.bin/tslint --project tsconfig.server.json --config tslint.json"
    },
    "author": "Jan Philip Schellenberg",
    "license": "",
    "dependencies": {
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.18.2",
        "changes-stream": "^2.2.0",
        "chart.js": "^2.7.2",
        "classnames": "2.2.5",
        "compression": "^1.7.1",
        "debug": "2.3.3",
        "express": "^4.16.0",
        "express-fileupload": "^0.4.0",
        "express-react-views": "^0.10.5",
        "http-proxy": "^1.16.2",
        "immutable": "^3.8.2",
        "jwt-simple": "^0.5.1",
        "lodash": "4.15.0",
        "markdown-it": "8.4.0",
        "material-ui": "0.16.7",
        "material-ui-chip-input": "0.16.0",
        "material-ui-color-picker": "^2.1.1",
        "mkdir": "0.0.2",
        "mkdirp": "^0.5.1",
        "moment": "^2.20.1",
        "moment-timezone": "^0.5.14",
        "nano": "^6.4.2",
        "query-string": "^5.0.1",
        "raven": "^2.4.0",
        "raven-for-redux": "^1.2.0",
        "raven-js": "^3.22.1",
        "react": "15.4.0",
        "react-chartjs-2": "^2.7.2",
        "react-chartkick": "^0.3.0",
        "react-dom": "15.4.0",
        "react-dropzone": "^4.2.3",
        "react-hot-loader": "1.3.0",
        "react-redux": "4.4.5",
        "react-router": "3.0.0",
        "react-router-redux": "4.0.7",
        "react-sortable-hoc": "0.6.7",
        "react-tap-event-plugin": "2.0.1",
        "recursive-readdir": "^2.2.2",
        "redux": "3.5.2",
        "redux-thunk": "2.1.0",
        "shortid": "2.2.8",
        "socket.io": "^2.0.4",
        "source-map-loader": "^0.2.3",
        "sticky-session": "^1.1.2",
        "superagent": "3.5.2",
        "unzip": "^0.1.11",
        "url-parse": "1.1.9"
    },
    "devDependencies": {
        "@types/classnames": "0.0.32",
        "@types/express": "4.0.37",
        "@types/express-serve-static-core": "4.0.49",
        "@types/lodash": "4.14.50",
        "@types/material-ui": "0.16.47",
        "@types/node": "7.0.2",
        "@types/raven": "^2.1.4",
        "@types/raven-js": "^3.10.0",
        "@types/react": "15.0.1",
        "@types/react-dom": "0.14.20",
        "@types/react-redux": "4.4.35",
        "@types/react-tap-event-plugin": "0.0.30",
        "@types/socket.io": "^1.4.31",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-es2015": "^6.24.1",
        "copy-webpack-plugin": "^4.5.2",
        "debug": "^2.3.3",
        "enzyme": "^3.2.0",
        "enzyme-adapter-react-15.4": "^1.0.5",
        "enzyme-to-json": "^3.2.2",
        "file-loader": "0.9.0",
        "html-webpack-plugin": "^2.30.1",
        "jest": "^21.2.1",
        "jest-cli": "^21.2.1",
        "react-addons-test-utils": "^15.6.2",
        "ts-jest": "^21.2.3",
        "ts-loader": "2.3.7",
        "tslint": "^5.7.0",
        "tslint-config-airbnb": "^5.3.1",
        "tslint-config-prettier": "^1.6.0",
        "tslint-loader": "^3.5.3",
        "tslint-react": "3.2.0",
        "typescript": "2.1.5",
        "uglifyjs-webpack-plugin": "^1.0.1",
        "webpack": "^3.8.1",
        "webpack-dev-server": "2.9.4"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(src/*/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ],
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "setupTestFrameworkScriptFile": "./src/client/utils/test-setup.ts"
    }
}
